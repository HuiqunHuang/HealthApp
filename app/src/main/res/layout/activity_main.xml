<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingLeft="0dp"
    android:paddingRight="0dp"
    android:paddingTop="0dp"
    tools:context="health.healthapp.Activity.MainActivity">

    <include layout="@layout/top"></include>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="164dp"
        android:id="@+id/fragment_main">

        <fragment
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:name="health.healthapp.Fragment.MainFragment"
            android:id="@+id/frag_main" />

        <fragment
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:name="health.healthapp.Fragment.HealthFragment"
            android:id="@+id/frag_health" />

        <fragment
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:name="health.healthapp.Fragment.MineFragment"
            android:id="@+id/frag_mine" />

    </FrameLayout>



</RelativeLayout>
